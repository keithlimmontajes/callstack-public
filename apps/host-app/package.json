{
  "name": "host",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "bootstrap": "yarn && yarn ios:bundle:exec",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean:node_modules": "rm -rf ./node_modules",
    "start": "react-native webpack-start",
    "build": "webpack -c webpack.config.js --env platform=android --env mode=production",
    "build:watch": "yarn build --watch",
    "bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev false",
    "bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js --dev false",
    "pods": "(cd ios && bundle install && bundle exec pod install)",
    "pods:update": "(cd ios && bundle exec pod update)",
    "ios:bundle:exec": "bundle install && cd ios && bundle exec pod install"
  },
  "dependencies": {
    "@candlefinance/faster-image": "1.3.3",
    "@global/components": "*",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.10",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@react-navigation/stack": "^6.3.20",
    "@redux/landers": "*",
    "@redux/root": "*",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "@superapp/config": "*",
    "@superapp/test-ids": "*",
    "@superapp/theme": "*",
    "@types/jest": "^29.5.12",
    "enzyme-to-json": "^3.6.2",
    "formik": "^2.4.5",
    "path": "^0.12.7",
    "query-string": "9.0.0",
    "react": "18.2.0",
    "react-native": "0.72.11",
    "react-native-actions-sheet": "^0.9.3",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-select-dropdown": "^3.4.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^15.0.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "9.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@callstack/repack": "3.7.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.12",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-snap-carousel": "^3.8.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.9",
    "prettier": "^3.0.1",
    "react-test-renderer": "18.2.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "4.8.4",
    "webpack": "5.90.3"
  },
  "federatedDependencies": [
    {
      "name": "Landers",
      "type": "internal",
      "version": "0.0.1",
      "url": "<url here>"
    }
  ],
  "jest": {
    "preset": "react-native"
  },
  "parserOptions": {
    "requireConfigFile": false
  }
}
